{% extends '::base.html.twig' %}

{% block title %}Habilitation{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
    '@FrontOfficeMainBundle/Resources/public/css/jquery-ui-1.10.4.custom.min.css'
    '@FrontOfficeMainBundle/Resources/public/css/logo.css'
    '@FrontOfficeMainBundle/Resources/public/css/placeholder.css'
    '@BackOfficeMonitoringBundle/Resources/public/css/monito.css'
    '@BackOfficeHabilitationBundle/Resources/public/css/od.css'
    '@BackOfficeHabilitationBundle/Resources/public/css/demo_table.css'
    %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
    {% endstylesheets %}
{% endblock %}

{% block nav_content %}
    {% if is_granted('ROLE_ADMIN') or is_granted('ROLE_ASSISTANCE') or is_granted('ROLE_RCCI')%}
        {% set route = app.request.attributes.get('_route') %}
        <h1>Module d'habilitation SAB</h1>
        <ul class="list-inline">
        </ul>
    {% endif %}
{% endblock %}

{% block body %}
    {{ parent() }}
    {% block content %}{% endblock %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts
    '@FrontOfficeMainBundle/Resources/public/js/jquery-2.1.0.min.js'
    '@FrontOfficeMainBundle/Resources/public/js/jquery-ui-1.10.4.custom.min.js'
    '@FrontOfficeMainBundle/Resources/public/js/bootstrap.min.js'
    '@FrontOfficeMainBundle/Resources/public/js/datePicker.js'
    '@FrontOfficeMainBundle/Resources/public/js/jquery.placeholder.js'
    '@BackOfficeHabilitationBundle/Resources/public/js/ajaxCall.js'
    '@BackOfficeHabilitationBundle/Resources/public/js/main.js'
    '@BackOfficeHabilitationBundle/Resources/public/js/detailJS.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}